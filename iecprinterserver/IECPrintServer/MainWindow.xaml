<Window x:Class="IECPrintServer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IECPrintServer"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="40" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal">
            <Button Margin="10" Width="100" Click="Button_Click_FF">FORM FEED</Button>
            <Button Margin="10" Width="100" Click="Button_Click_SAVE">SAVE</Button>
            <Button x:Name="btnLPI" Margin="10" Width="100" Click="Button_Click_LPI">8 LPI</Button>
            <Button x:Name="btnQuality" Margin="10" Width="100" Click="Button_Click_Quality">DRAFT</Button>
        </StackPanel>
        <ScrollViewer Margin="10" Grid.Row="2" >
            <Image x:Name="OutputImage" Stretch="UniformToFill" Margin="0"/>

        </ScrollViewer>

    </Grid>
</Window>
